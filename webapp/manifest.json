{
    "_version": "1.65.0",
    "sap.app": {
        "id": "com.inv.sapfioriwebinversion",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.16.5",
            "toolsId": "7a9d7b5d-5adc-47b8-9ec5-f50ce2dd071a"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.130.6",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.tnt": {},
                "sap.f": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.inv.sapfioriwebinversion.i18n.i18n"
                }
            },
            "dataModel": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {
                    "dataSource": "salesForecast"
                }
            },
            "dataSources": {
                "salesForecast": {
                    "uri": "resources/salesforecast.json",
                    "type": "JSON"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "controlAggregation": "pages",
                "controlId": "app",
                "transition": "slide",
                "type": "View",
                "viewType": "XML",
                "path": "com.inv.sapfioriwebinversion.view",
                "async": true,
                "viewPath": "com.inv.sapfioriwebinversion.view"
            },
            "routes": [
                {
                    "name": "RouteMain",
                    "pattern": "home",
                    "target": ["TargetMain"]
                },
                {
                    "name": "RouteSalesForecast",
                    "pattern": "salesforecast",
                    "target": ["TargetSalesForecast"]
                },
                {
                    "name": "RouteLogin",
                    "pattern": "login",
                    "target": ["TargetLogin"]
                }
            ],
            "targets": {
                "TargetMain": {
                    "id": "Main",
                    "name": "Main"
                },
                "TargetSalesForecast": {
                    "id": "SalesForecast",
                    "name": "SalesForecast",
                    "viewName": "salesforecast.SalesForecast",
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewLevel": 3,
                    "path": "com.inv.sapfioriwebinversion.view.salesforecast",
                    "viewPath": "com.inv.sapfioriwebinversion.view.salesforecast"
                }
            }
        },
        "rootView": {
            "viewName": "com.inv.sapfioriwebinversion.view.App",
            "type": "XML",
            "id": "App"
        }
    }
}
