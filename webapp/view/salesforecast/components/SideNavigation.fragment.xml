    xmlns:core="sap.ui.core"
    xmlns:tnt="sap.tnt">
    
    <tnt:SideNavigation id="IdSideNavigation1SalesForecast"
        expanded="true"
        selectedKey="{salesForecastNavModel>/selectedKey}"
        itemSelect="onItemSelect">
        
